package main

var a string
var c = make(chan int)

func f(){
  a = "hello, wordld"
	<-c //取出缓冲区数据
}

func main() {
	go f()
	c <-0//清空C的缓冲区 
}


----------------------------------------------------
var a string
var c = make(chan int, 10)

func f(){
	a ="hello, word"
	c<-0 //先清空缓冲区
}

func main(){
	go f()
	<-c //取出C中数据
	print(a)
}
